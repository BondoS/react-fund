{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    // "prettier/prettier": "error",
    "space-before-function-paren": "off",
    "space-before-blocks": "off",
    "func-names": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
